{
  "name": "adoption-plant-react",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "pnpm gen:abi && vite --open",
    "dev:test": "vite --mode=mumbai --open",
    "build": "vite build",
    "build:test": "vite build --mode=mumbai",
    "gen:api": "ts-node --esm openapi.config.ts",
    "gen:abi": "typechain --target ethers-v6 --out-dir src/abis/types 'src/abis/*.json'",
    "commit": "wave commit",
    "release": "release-it",
    "report": "vite build --mode=REPORT",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^1.3.2",
    "antd": "5.9.4",
    "axios": "^1.6.3",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.10",
    "ethers": "^6.11.1",
    "i18next": "^23.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.21.1",
    "viem": "^1.21.1",
    "wagmi": "^1.4.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/bignumber.js": "^5.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@unocss/eslint-plugin": "^0.58.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "openapi-genuu": "^0.1.6",
    "release-it": "^17.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typechain": "^8.3.2",
    "typescript": "^5.2.2",
    "unocss": "^0.58.0",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1"
  }
}
